<%= link_to 'Back', schools_dashboard_path, class: 'govuk-back-link' %>

<%= form_for @placement_date, url: schools_placement_date_path(@placement_date), method: :put do |form| %>
  <%= render partial: 'form', locals: { form: form } %>
  <%= form.submit 'Modify placement date' %>
<%- end -%>

<%= button_to 'Delete',
  schools_placement_date_path,
  method: 'delete',
  class: 'govuk-button govuk-button--secondary'
%>
